#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here

# Do the setup
# initialize
board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]

puts "Welcome to Tic Tac Toe!"

# Greet the user
puts "Where would you like to go?"

# Get & apply the move
input = gets.strip
index = input_to_index(input)
move(board,index,"X")
display_board